@model IEnumerable<MeetingRoomWebApp.AutoGen.User>
@{
    ViewBag.Title = "User List";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>User List</h2>

<table class="table">
    <thead>
        <tr style="background-color: rgb(9, 9, 51); color: yellow ">
            <th>ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Role</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserId</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@user.Role</td>
            </tr>
        }
    </tbody>
</table>
